<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Startgame_controller">
   <children>
      <ImageView fitHeight="1000.0" fitWidth="1000.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Mainmenu.jpeg" />
         </image>
      </ImageView>
      <ChoiceBox fx:id="chooselevel" layoutX="596.0" layoutY="395.0" prefHeight="23.0" prefWidth="142.0" />
      <Label layoutX="376.0" layoutY="395.0" prefHeight="39.0" prefWidth="157.0" text="Choose Level" />
      <Button fx:id="resume" layoutX="376.0" layoutY="297.0" mnemonicParsing="false" prefHeight="4.0" prefWidth="118.0" text="Resume" />
      <Label fx:id="message" layoutX="596.0" layoutY="301.0" prefHeight="31.0" prefWidth="118.0" text="Message" />
      <Button fx:id="startgame" layoutX="376.0" layoutY="508.0" mnemonicParsing="false" text="Start Game" />
   </children>
</AnchorPane>
